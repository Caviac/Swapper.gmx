//Input
if (global.control == true){
    if (buttonLeft and (mostRecentDirection == "left")){
        hSpeed = -(sideSpeed);
    }else if (buttonRight and (mostRecentDirection == "right")){
        hSpeed = sideSpeed;
    }else{
        hSpeed = 0;
    }
}else{
    hSpeed = 0
}

//Ceilings
if (!place_free(x, y - 1)){
    vSpeed = 0
}

//Gravity
if (place_free(x, y + 1)){
    if (vSpeed &lt; grav){
        for (i = 0; i &lt; accel; i++){
            vSpeed++
        }
    }
}else{
    vSpeed = 0;
    jumpTest = true;
}

//Jumping
if (buttonJump and jumpTest and !(place_free(x, y+1))){
    vSpeed = -(jumpSpeed);
    jumpTest = false
}

//Apply Movement
for (i = 0; i &lt; abs(hSpeed); i++){
    if place_free(x + sign(hSpeed), y){
        x += (sign(hSpeed));
    }
    
}

for (i = 0; i &lt; abs(vSpeed); i++){
    if place_free(x, y + sign(vSpeed)){
        y += (sign(vSpeed));
    }
    
}
